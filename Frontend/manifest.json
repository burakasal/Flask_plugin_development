{
    "name": "Hello Extensions",
    "description": "Base Level Extension", 
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["activeTab", "storage", "declarativeContent", "tabs", "<all_urls>"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "NLP.png"
    }
}